{
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.importModuleSpecifier": "auto",
  "files.associations": {
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".stylelintrc": "jsonc",
    ".optipackrc": "jsonc",
    ".renovaterc": "jsonc",
    "*.ejs.t": "html"
  },
  "stylelint.enable": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "json.schemas": [
    {
      "fileMatch": [".renovaterc"],
      "url": "http://docs.renovatebot.com/renovate-schema.json"
    }
  ]
}
